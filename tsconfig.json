{
  "compilerOptions": {
    "outDir": "./dist",
    "pretty": true,
    "target": "es2015",
    "lib": [
      "es2015",
      "es2016.array.include",
    ],
    "module": "commonjs",
    "allowJs": false,
    "sourceMap": true,
    "strict": true,
    // "declaration": true,
    "importHelpers": true,
    "noImplicitAny": false // Needed to compile tap and ansi-escapes
  },
  "include": [
      "./lib/**/*"
  ]
}
